#include<iostream>
using namespace std;
void segregate(int arr[],int n)
{
int newarr[n],q=0,p=0;
int j=0;
for(int i=0;i<n;i++)
{
   if(arr[i]<0)
   {
    newarr[j]=arr[i];
    j++;
   }
}
for(int i=0;i<n;i++)
{
    if(arr[i]>=0){
        newarr[j]=arr[i];
        j++;
    }
}
 for(int i=0;i<n;i++)
 {
     arr[i]=newarr[i];
 }
}
int main()
{
    int n;
    cout<<"enter size";
    cin>>n;
    int arr[n];
    cout<<"enter elements";
    for(int i=0;i<n;i++)
    {
cin>>arr[i];
    }
    segregate(arr,n);
     for(int i=0;i<n;i++)
    {
cout<<arr[i]<<" ";
    }
    return 0;
}
