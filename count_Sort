#include<iostream>
#include<bits/stdc++.h>
using namespace std;
void Count(int arr[],int n)
{
int count[20]={0},mini=arr[0];
for(int i=0;i<n;i++)
{
    if(arr[i]<mini)
    {
        mini=arr[i];
    }
}
mini=abs(mini);
for(int i=0;i<n;i++)
{
arr[i]=arr[i]+mini;
}
for(int i=0;i<n;i++)
{
    count[arr[i]]=count[arr[i]]+1;
}
for(int i=0;i<19;i++)
{
    count[i+1]=count[i+1]+count[i];
}
for(int i=0;i<=19;i++)
{
    cout<<count[i]<<" ";
}
cout<<endl;
int b[n];
for(int i=n-1;i>=0;i--)
{
    count[arr[i]]=count[arr[i]]-1;
    b[count[arr[i]]]=arr[i];
}
for(int i=0;i<n;i++)
{
    b[i]=b[i]-mini;
}
for(int i=0;i<n;i++)
    {
        arr[i]=b[i];
    }
}
int main()
{
    int arr[50],n;
    cout<<"enter size";
    cin>>n;
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    Count(arr,n);
    for(int i=0;i<n;i++)
    {
        cout<<arr[i]<<" ";
    }
    return 0;
}
